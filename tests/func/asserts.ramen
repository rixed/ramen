DEFINE parent as
  yield random - 1 as x,
        random as y
  EVERY 1s;

DEFINE func AS
  select x / y AS
    x_per_y,
    x,
    now as start,
    now as stop
  FROM parent
  Preconditions
    "y not null":in.y <> 0,
    in.x >= 0
  Postconditions
    "condition" :out.x > 0,
    out.x_per_y > 0;
