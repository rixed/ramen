[Unit]
Description=Precompile ramen programs
PartOf=ramen.target
Documentation=https://rixed.github.io/ramen/man/precompserver.html

[Install]
WantedBy=ramen.target

[Service]
EnvironmentFile=-/etc/default/ramen
ExecStart=/usr/bin/ramen precompserver --stdout
User=ramen
Group=ramen
Restart=always
RestartSec=60
